# Sample environment variables for the application
# Copy this file to .env and update the values

# API Configuration
REACT_APP_API_URL=https://victoryline.live/leads-api

# Google Apps Script URL for form submissions
# Replace with your actual deployment URL after setting up the Google Apps Script
REACT_APP_GOOGLE_SCRIPT_URL=https://script.google.com/macros/s/YOUR_DEPLOYMENT_ID_HERE/exec

# Development Settings
# Set to 'true' to use mock API responses during development
REACT_APP_MOCK_API=false 